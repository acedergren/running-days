{
  "name": "@running-days/business-logic",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./workout": {
      "types": "./dist/workout-processor.d.ts",
      "import": "./dist/workout-processor.js"
    },
    "./goal": {
      "types": "./dist/goal-processor.d.ts",
      "import": "./dist/goal-processor.js"
    },
